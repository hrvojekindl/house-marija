							<div class="carousel-caption">
								<h1><%= replace "head3" %></h1>
								<p class="lead"><%= replace "lead3" %></p>
							</div>
						</div>
					</div>
				</div>
				<a class="left carousel-control" href="#carousel" data-slide="prev">&lsaquo;</a>
				<a class="right carousel-control" href="#carousel" data-slide="next">&rsaquo;</a>
			</div>
		</div>

		<!-- ABOUT -->
		<section id="about">
			<div class="container">
				<h3><%= replace "about" %></h3>
				<div class="row-fluid">
					<p class="span8"><%= replace "about-text" %></p>
					<div class="well well-small span4">
						<span class="bullets-title"><%= replace "house-bullet-title" %></span>
						<ul class="bullets">
							<li><%= replace "house-bullet1" %></li>
							<li><%= replace "house-bullet2" %></li>
							<li><%= replace "house-bullet3" %></li>
							<li><%= replace "house-bullet4" %></li>
							<li><%= replace "house-bullet5" %></li>
							<li><%= replace "house-bullet6" %></li>
							<li><%= replace "house-bullet7" %></li>
							<li><%= replace "house-bullet8" %></li>
							<li><%= replace "house-bullet9" %></li>
						</ul>
					</div>
				</div>
				<div class="row-fluid">
					<ul class="elastislide-list" data-rel="about" data-image-set="about" data-image-count="13"></ul>
				</div>
			</div>
		</section>

